<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	
	<package name="studentInfo" namespace="/" extends="struts-default">
		<action name="student" class="action.StudentAction">
			<result name="success">/S_Page/S_main.jsp</result>
			<result name="error">/S_Page/S_login.jsp</result>
			<result name="logout" type="redirect">/S_Page/S_login.jsp</result>
			<result name="turnto">{mainPage}</result>
			<result name="save" type="redirectAction">student!list</result>
		</action>
		
		
	</package>
	
	<package name="p_action" namespace="/" extends="struts-default">
	  <action name="ad" class="p_action.Test_p_1">
	  <result name="ok" ></result>
	  </action>
	</package>
	

     
</struts>